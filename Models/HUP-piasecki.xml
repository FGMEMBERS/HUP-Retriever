        <!--===========SIEGE COPILOTE et TRAPPE============================-->

        <animation>
            <type>rotate</type>
             <object-name>DSiege-Base</object-name>
             <object-name>DSiege-H</object-name>
             <object-name>DSiege-Bras</object-name>
             <object-name>DSiege-V</object-name>
            <property>surface-positions/trap-position</property>
            <interpolation>
                    <entry>
                    <ind>0.0</ind>
                    <dep>0</dep>
                    </entry>
                    <entry>
                    <ind>0.33</ind>
                    <dep>0</dep>
                    </entry>
                    <entry>
                    <ind>0.66</ind>
                    <dep>-90</dep>
                    </entry>
                    <entry>
                    <ind>1.0</ind>
                    <dep>-90</dep>
                    </entry>
            </interpolation>
            <center>
                <x-m>-3.88295</x-m>
                <y-m>0</y-m>
                <z-m>-0.85784</z-m>
            </center>
            <axis>
              <x>0</x>
              <y>1</y>
              <z>0</z>
            </axis>
        </animation>

